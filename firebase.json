{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": [
    {
      "target": "Workout-Planner",
      "public": "dist/Workout-Planner",
      "ignore": [
        "firebase.json",
        ".firebaserc",
        ".vscode",
        ".git",
        ".gitignore",
        ".editorconfig",
        "src/**/.*",
        "database.rules.json",
        "package.json",
        "README.md",
        "tsconfig.json",
        "webpack.config.js",
        "yarn.lock",
        "**/node_modules/**",
        "**/.angular/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}